{
  "name": "xiaoguo-desktop",
  "version": "1.2.39",
  "description": "大嘴外语小果AI",
  "author": "毛毛 <962212011@qq.com>",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "release": "electron-builder --win --x64 --publish=never"
  },
  "dependencies": {
    "electron-log": "^5.4.1",
    "electron-updater": "^6.6.2",
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^26.0.16"
  },
  "build": {
    "appId": "com.xiaoguoai.desktop",
    "productName": "大嘴外语晓果AI",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "main.js",
      "package.json",
      "assets/**/*"
    ],
    "extraResources": [
      {
        "from": "assets/",
        "to": "assets"
      }
    ],
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://mmapi.jxphp.com/static/xiaoguo-desktop/"
      }
    ]
  }
}
